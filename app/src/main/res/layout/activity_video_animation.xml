<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VideoAnimation">

    <ImageView
        android:id="@+id/imageViewBoy5"
        android:layout_width="64dp"
        android:layout_height="61dp"
        android:layout_marginBottom="596dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/boyrunningn" />

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="348dp"
        android:layout_height="293dp"
        android:layout_marginBottom="288dp"
        app:layout_constraintBottom_toTopOf="@+id/imageViewAnimHome"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageViewAnimPre"
        android:layout_width="75dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back" />

    <ImageView
        android:id="@+id/imageViewAnimHome"
        android:layout_width="53dp"
        android:layout_height="41dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageViewAnimFwd"
        app:layout_constraintStart_toEndOf="@+id/imageViewAnimPre"
        app:srcCompat="@drawable/home" />

    <ImageView
        android:id="@+id/imageViewAnimFwd"
        android:layout_width="59dp"
        android:layout_height="42dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/forward" />

</androidx.constraintlayout.widget.ConstraintLayout>